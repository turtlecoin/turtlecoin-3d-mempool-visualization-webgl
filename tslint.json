{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single"],
    "no-console": [true, "never"],
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never"
        },
        "esSpecCompliant": true
      }
    ],
    "no-default-export": true
  },
  "rulesDirectory": []
}
